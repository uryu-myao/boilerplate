    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "concurrently": "^7.3.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.19"
  }
}
